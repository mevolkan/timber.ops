{% if menu %}
	<ul class="{{ item.classes | join(' ') }} nav__list"> 
	{% for item in menu %}
		<li class="{{ item.classes | join(' ' ) }} nav__item ">
			<a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
			{% include "menu.twig" with {'menu': item.get_children} %}
		</li>
	{% endfor %}
	</ul>
{% endif %}

